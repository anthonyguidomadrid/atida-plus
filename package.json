  "scripts": {
    "dev": "run-p dev:**",
    "dev:next": "TAILWIND_MODE=watch node -r @newrelic/next server.js | pino-pretty",
    "dev:storybook": "start-storybook -p 6006",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "build:profile": "yarn build --profile",
    "build:storybook": "build-storybook",
    "start": "NODE_ENV=production node -r @newrelic/next server.js",
    "test": "jest --coverage --verbose --color",
    "test:watch": "jest --watch",
    "lint": "eslint --resolve-plugins-relative-to ./ --ext js,jsx,ts,tsx ./",
    "lint:fix": "yarn lint --fix",
    "cypress": "cypress run",
    "cypress:es": "cypress run --env locale='es'",
    "cypress:uat": "cypress run --config-file cypress.uat.config.js",
    "cypress:account": "CYPRESS_TEAM='account' cypress run",
    "cypress:basket-checkout": "CYPRESS_TEAM='basket-checkout' cypress run",
    "cypress:core-shopping": "CYPRESS_TEAM='core-shopping' cypress run",
    "cypress:nav-search-ranking": "CYPRESS_TEAM='nav-search-ranking' cypress run",
    "cypress:open": "cypress open --e2e --browser chrome",
    "cypress:open:es": "cypress open --e2e --browser chrome --env locale='es'",
    "cypress:local": "cypress run --config-file cypress.local.config.js",
    "cypress:api": "cypress run --config-file cypress.api.config.js",
    "cypress:open:uat": "cypress open --e2e --browser chrome --config-file cypress.uat.config.js",
    "cypress:open:uat:es": "cypress open --e2e --browser chrome --config-file cypress.uat.config.js --env locale='es'",
    "cypress:account:uat": "CYPRESS_TEAM='account' cypress run --config-file cypress.uat.config.js",
    "cypress:basket:uat": "CYPRESS_TEAM='basket-checkout' cypress run --config-file cypress.uat.config.js",
    "cypress:basket:uat:es": "CYPRESS_TEAM='basket-checkout' cypress run --config-file cypress.uat.config.js --env locale='es'",
    "cypress:account:uat:es": "CYPRESS_TEAM='account' cypress run --config-file cypress.uat.config.js --env locale='es'",
    "cypress:local:account": "CYPRESS_TEAM='account' cypress run --config-file cypress.local.config.js",
    "cypress:local:basket-checkout": "CYPRESS_TEAM='basket-checkout' cypress run --config-file cypress.local.config.js",
    "cypress:local:core-shopping": "CYPRESS_TEAM='core-shopping' cypress run --config-file cypress.local.config.js",
    "cypress:local:nav-search-ranking": "CYPRESS_TEAM='nav-search-ranking' cypress run --config-file cypress.local.config.js",
    "cypress:local:open": "cypress open --e2e --browser chrome --config-file cypress.local.config.js",
    "generate-graphql": "cross-env DOTENV_CONFIG_PATH=\"./.env.local\" graphql-codegen --require dotenv/config --config codegen.yml",
    "lighthouse": "lhci autorun",
    "verify": "yarn tsc && yarn lint && yarn test",
    "extract-translations": "cross-env NODE_ENV=extract_translations babel -f .babelrc.js 'src/**/!(*.test|*.stories).{ts,tsx}'",
    "postinstall": "patch-package"
  },
